<script>
  import IngishtsTracker from "insights-kit";
  window.onload = async () => {
    window.Insights = new IngishtsTracker({
      trackingHost: import.meta.env.PUBLIC_INSIGHTS_HOST,
    });
    await window.Insights.createSession();
    // start page tracking here
    window.Insights.track({
      type: "page-views",
    });
  };
</script>
